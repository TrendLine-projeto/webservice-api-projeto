        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT",
                "description": "Token de autenticação Bearer. Coloque o token JWT gerado na caixa abaixo no formato `Bearer <token>`."
            }
        },
        "schemas": {
            "usuarioLogin": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string",
                        "description": "Nome de usuário",
                        "example": "SunnyAura.suporte"
                    },
                    "senha": {
                        "type": "string",
                        "description": "Senha do usuário",
                        "example": "123456"
                    }
                }
            },
            "usuarioCreate": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string",
                        "description": "Nome de usuário",
                        "example": "SunnyAura.novo"
                    },
                    "senha": {
                        "type": "string",
                        "description": "Senha do usuário",
                        "example": "minhasenha"
                    },
                    "email": {
                        "type": "string",
                        "description": "Email do usuário",
                        "example": "novo@email.com"
                    }
                }
            },
            "usuarioAutenticado": {
                "type": "object",
                "properties": {
                    "usuario": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "integer",
                                "example": 1
                            },
                            "username": {
                                "type": "string",
                                "example": "SunnyAura.suporte"
                            },
                            "email": {
                                "type": "string",
                                "example": "sunnyaura@gmail.com"
                            },
                            "cliente": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "razaoSocial": {
                                        "type": "string",
                                        "example": "Sunny Aura"
                                    },
                                    "nomeFantasia": {
                                        "type": "string",
                                        "example": "Sunny Aura"
                                    },
                                    "cnpj": {
                                        "type": "string",
                                        "example": "46.095.984/0001-32"
                                    },
                                    "filiais": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "integer"
                                                },
                                                "nomeFilial": {
                                                    "type": "string"
                                                },
                                                "endereco": {
                                                    "type": "string"
                                                },
                                                "cidade": {
                                                    "type": "string"
                                                },
                                                "estado": {
                                                    "type": "string"
                                                },
                                                "cep": {
                                                    "type": "string"
                                                },
                                                "telefone": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            "menus": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "integer"
                                        },
                                        "nome": {
                                            "type": "string"
                                        },
                                        "icone": {
                                            "type": "string"
                                        },
                                        "link": {
                                            "type": "string"
                                        },
                                        "submenus": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "integer"
                                                    },
                                                    "nome": {
                                                        "type": "string"
                                                    },
                                                    "link": {
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
