                "summary": "Cadastrar novo produto de suprimentos",
                "description": "Cria um novo produto de suprimentos associado a um fornecedor.",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "nomeProduto": {
                                        "type": "string",
                                        "example": "Caixa de Brinquedos"
                                    },
                                    "descricao": {
                                        "type": "string",
                                        "example": "Contém 10 unidades de brinquedos variados"
                                    },
                                    "codigoInterno": {
                                        "type": "string",
                                        "example": "BRINQ-001"
                                    },
                                    "codigoFornecedor": {
                                        "type": "string",
                                        "example": "HAS-BOX-2025"
                                    },
                                    "unidadeMedida": {
                                        "type": "string",
                                        "example": "CX"
                                    },
                                    "precoUnitario": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 150.00
                                    },
                                    "estoqueMinimo": {
                                        "type": "integer",
                                        "example": 5
                                    },
                                    "estoqueAtual": {
                                        "type": "integer",
                                        "example": 20
                                    },
                                    "ativo": {
                                        "type": "boolean",
                                        "example": true
                                    },
                                    "fornecedor_id": {
                                        "type": "integer",
                                        "example": 3
                                    }
                                },
                                "required": [
                                    "nomeProduto",
                                    "descricao",
                                    "fornecedor_id"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Produto de suprimentos criado com sucesso."
                    },
                    "400": {
                        "description": "Campos obrigatórios não preenchidos."
                    },
