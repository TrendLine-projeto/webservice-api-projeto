    "paths": {
        "/fornecedorProd/fornecedores_producao": {
            "post": {
                "tags": [
                    "Fornecedores Producao"
                ],
                "summary": "Cadastrar novo fornecedor de produção",
                "description": "Cria um novo fornecedor de produção associando-o a um cliente.",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "razaoSocial": {
                                        "type": "string",
                                        "example": "Hasbro do Brasil"
                                    },
                                    "nomeFantasia": {
                                        "type": "string",
                                        "example": "Hasbro"
                                    },
                                    "cnpj": {
                                        "type": "string",
                                        "example": "08.743.754/0004-96"
                                    },
                                    "inscricaoEstadual": {
                                        "type": "string",
                                        "example": "123456789"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "comercial@hasbro.com"
                                    },
                                    "telefone": {
                                        "type": "string",
                                        "example": "(11) 4002-8922"
                                    },
                                    "celular": {
                                        "type": "string",
                                        "example": "(11) 98888-7777"
                                    },
                                    "responsavel": {
                                        "type": "string",
                                        "example": "José da Silva"
                                    },
                                    "site": {
                                        "type": "string",
                                        "example": "https://www.hasbro.com"
                                    },
                                    "tipoFornecedor": {
                                        "type": "string",
                                        "example": "Nacional"
                                    },
                                    "categoria": {
                                        "type": "string",
                                        "example": "Brinquedos"
                                    },
                                    "endereco": {
                                        "type": "string",
                                        "example": "Rua Exemplo, 123"
                                    },
                                    "bairro": {
                                        "type": "string",
                                        "example": "Centro"
                                    },
                                    "cidade": {
                                        "type": "string",
                                        "example": "São Paulo"
                                    },
                                    "estado": {
                                        "type": "string",
                                        "example": "SP"
                                    },
                                    "cep": {
                                        "type": "string",
                                        "example": "01000-000"
                                    },
                                    "pais": {
                                        "type": "string",
                                        "example": "Brasil"
                                    },
                                    "observacoes": {
                                        "type": "string",
                                        "example": "Fornecedor homologado."
                                    },
                                    "ativo": {
                                        "type": "boolean",
                                        "example": true
                                    },
                                    "cliente_id": {
                                        "type": "integer",
                                        "example": 1
                                    }
                                },
                                "required": [
                                    "razaoSocial",
                                    "cnpj",
                                    "cliente_id"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Fornecedor criado com sucesso."
                    },
                    "400": {
                        "description": "Erro de validação"
                    },
                    "404": {
                        "description": "Cliente não encontrado"
                    },
                    "500": {
                        "description": "Erro interno do servidor"
                    }
                }
            }
        },
        "/fornecedorProd/fornecedores_producao/buscar": {
            "post": {
                "tags": [
                    "Fornecedores Producao"
                ],
                "summary": "Buscar fornecedores com filtros e paginação",
                "description": "Busca fornecedores de producao de um cliente, com suporte a filtros dinâmicos e paginação.",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "cliente_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "pagina": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "quantidadePorPagina": {
                                        "type": "integer",
                                        "example": 10
                                    },
                                    "razaoSocial": {
                                        "type": "string",
                                        "example": "Fornecedor A",
                                        "nullable": true
                                    },
                                    "cidade": {
                                        "type": "string",
                                        "example": "São Paulo",
                                        "nullable": true
                                    },
                                    "estado": {
                                        "type": "string",
                                        "example": "SP",
                                        "nullable": true
                                    },
                                    "ativo": {
                                        "type": "boolean",
                                        "example": true,
                                        "nullable": true
                                    },
                                    "tipoFornecedor": {
                                        "type": "string",
                                        "example": "Nacional",
                                        "nullable": true
                                    }
                                },
                                "required": [
                                    "cliente_id"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Fornecedores encontrados com sucesso."
                    },
                    "400": {
                        "description": "Cliente ID não informado."
                    },
                    "404": {
                        "description": "Nenhum fornecedor encontrado."
                    },
                    "500": {
                        "description": "Erro interno do servidor."
                    }
                }
            }
        },
